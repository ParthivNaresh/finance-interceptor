{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://finance-interceptor-api.onrender.com",
        "EXPO_PUBLIC_PLAID_ENVIRONMENT": "sandbox"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://finance-interceptor-api.onrender.com",
        "EXPO_PUBLIC_PLAID_ENVIRONMENT": "sandbox"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.financeinterceptor.com",
        "EXPO_PUBLIC_PLAID_ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
